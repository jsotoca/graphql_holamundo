{
  "name": "hola-mundo",
  "version": "1.0.0",
  "description": "Hola mundo en graphql",
  "main": "build/server.js",
  "scripts": {
    "start": "node build/server.js",
    "build": "tsc -p . && ncp src/schema build/schema",
    "start:dev": "npm run build:dev",
    "build:dev": "nodemon src/server.ts --exec ts-node src/server.ts -e ts,graphql"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsotoca/graphql_holamundo.git"
  },
  "keywords": [
    "graphql",
    "graphql",
    "server"
  ],
  "author": "Juan Antonio Soto Cabrera",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsotoca/graphql_holamundo/issues"
  },
  "homepage": "https://github.com/jsotoca/graphql_holamundo#readme",
  "dependencies": {
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.4.0",
    "graphql-import-node": "0.0.4",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-tools": "^7.0.2",
    "http": "0.0.1-security",
    "ncp": "^2.0.0",
    "typescript": "^4.1.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.8",
    "@types/express": "^4.17.9",
    "@types/express-graphql": "^0.9.0",
    "@types/graphql": "^14.5.0",
    "@types/node": "^14.14.10"
  }
}
